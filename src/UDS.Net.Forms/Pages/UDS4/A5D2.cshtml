                                                <div class="mt-4 sm:col-span-2 sm:mt-0">
                                                    <p class="text-sm text-gray-500" asp-description-for="@Model.A5D2.HRTATTMULT"></p>
                                                    <radio-button-group id="@Html.IdFor(m => m.A5D2.HRTATTMULT)" for="@Model.A5D2.HRTATTMULT" items="Model.BasicYesNoListItems"></radio-button-group>
