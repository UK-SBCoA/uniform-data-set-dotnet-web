                            <label asp-for="A5D2.ALCFREQYR"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5D2.ALCFREQYR)</br><span class="font-bold">(IF NEVER OR UNKNOWN, SKIP TO QUESTION 11)</span></label>
