@page "{id:int?}"
@model UDS.Net.Forms.Pages.UDS4.D1aModel
@{
    Layout = "_LayoutForm";
    string rowCSS = "sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6";
}

<div>
    <p class="mt-1 text-sm text-gray-500">
        INSTRUCTIONS: This form is to be completed by the clinician. For additional clarification and examples, see the <guidebook kind="@Model.Visit.PACKET"></guidebook>, Form D1a. Check only <span class="underline">one</span> box per question.
    </p>
</div>

<div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
    <label asp-for="D1a.DXMETHOD"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1a.DXMETHOD)</label>
    <div class="mt-4 sm:col-span-2 sm:mt-0">
        <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.DXMETHOD"></p>
        <radio-button-group id="@Html.IdFor(m => m.D1a.DXMETHOD)" for="@Model.D1a.DXMETHOD" items="Model.DiagnosisMethodListItems"></radio-button-group>
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.DXMETHOD"></span>
        <submission-error location="DXMETHOD" errors="Model.BaseForm.UnresolvedErrors" />
    </div>
</div>

<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <a name="section-1"></a>
    <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
        <h3 class="ml-2 mt-2 font-semibold text-sm text-gray-500 uppercase">Section 1</h3>
        <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
            Level of impairment - <i>Unimpaired cognition/behavior, SCD, MCI/MBI, or dementia</i>
        </p>
    </div>

    <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
        <label asp-for="D1a.NORMCOG"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1a.NORMCOG)</label>
        <div class="mt-4 sm:col-span-2 sm:mt-0">
            <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.NORMCOG"></p>
            <radio-button-group id="@Html.IdFor(m => m.D1a.NORMCOG)" for="@Model.D1a.NORMCOG" items="Model.NORMCOGListItems" ui-behaviors="Model.NORMCOGUIBehavior"></radio-button-group>
            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.NORMCOG"></span>
            <submission-error location="NORMCOG" errors="Model.BaseForm.UnresolvedErrors" />
        </div>
        <span class="italic text-gray-500">
            Note: For those with longstanding cognitive impairment that does not represent a decline from their usual functioning, consider
            checking <strong>Question 5b</strong> for a diagnosis of <strong>“Cognitively Impaired, Not MCI/dementia”</strong> .
        </span>
    </div>
</div>

<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
        <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
            Subjective Cognitive Decline
        </p>
    </div>
    <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6 subcounterreset">
        <label asp-for="D1a.SCD"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1a.SCD)</label>
        <div class="mt-4 sm:col-span-2 sm:mt-0">
            <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.SCD"></p>
            <radio-button-group id="@Html.IdFor(m => m.D1a.SCD)" for="@Model.D1a.SCD" items="Model.SCDListItems" ui-behaviors="Model.SCDUIBehavior"></radio-button-group>
            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.SCD"></span>
            <submission-error location="SCD" errors="Model.BaseForm.UnresolvedErrors" />
        </div>
    </div>
    <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
        <label asp-for="D1a.SCDDXCONF"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1a.SCDDXCONF)</label>
        <div class="mt-4 sm:col-span-2 sm:mt-0">
            <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.SCDDXCONF"></p>
            <radio-button-group id="@Html.IdFor(m => m.D1a.SCDDXCONF)" for="@Model.D1a.SCDDXCONF" items="Model.SCDDXCONFListItems" ui-behaviors="Model.SCDDXCONFUIBehavior"></radio-button-group>
            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.SCDDXCONF"></span>
            <submission-error location="SCDDXCONF" errors="Model.BaseForm.UnresolvedErrors" />
        </div>
    </div>
</div>

<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
        <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
            Dementia criteria
        </p>
    </div>
    <div class="sm:grid sm:grid-cols-2 sm:items-start sm:gap-4 sm:py-6">
        <div class="p-4 bg-white rounded-lg">
            <h3 class="font-bold text-lg text-indigo-900 mb-2">Requirement #1:</h3>
            <p class="text-sm font-semibold">Participant has cognitive or behavioral (neuropsychiatric) symptoms that meet all of the following criteria:</p>
            <ul class="list-disc pl-4">
                <li>Interfere with ability to function as before at work or at usual activities</li>
                <li>Represent a decline from previous levels of functioning</li>
                <li>Are not explained by delirium or major psychiatric disorder</li>
                <li>Include cognitive impairment detected and diagnosed through a combination of: 1) history-taking; 2) objective assessment (bedside or neuropsychological testing)</li>
            </ul>
        </div>

        <div class="p-4 bg-white rounded-lg">
            <h3 class="font-bold text-lg text-indigo-900 mb-2">Requirement #2:</h3>
            <p class="text-sm font-semibold">Participant must have impairment in one* or more of the following domains:</p>
            <ul class="list-disc pl-4">
                <li>Impaired ability to acquire and remember new information</li>
                <li>Impaired reasoning and handling of complex tasks, poor judgment</li>
                <li>Impaired visuospatial abilities</li>
                <li>Impaired language functions</li>
                <li>Changes in personality, behavior, or comportment</li>
            </ul>
            <p class="text-xs italic pl-4">*In the event of single-domain impairment (e.g., language in PPA, behavior in bvFTD, visuospatial in posterior cortical atrophy, etc.), the participant must not fulfill criteria for MCI.</p>
        </div>
    </div>

</div>


<div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
    <label asp-for="D1a.DEMENTED"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1a.DEMENTED)</label>
    <div class="mt-4 sm:col-span-2 sm:mt-0">
        <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.DEMENTED"></p>
        <radio-button-group id="@Html.IdFor(m => m.D1a.DEMENTED)" for="@Model.D1a.DEMENTED" items="Model.DEMENTEDListItems" ui-behaviors="Model.DEMENTEDUIBehavior"></radio-button-group>
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.DEMENTED"></span>
        <submission-error location="DEMENTED" errors="Model.BaseForm.UnresolvedErrors" />
    </div>
</div>

<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
        <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
            MCI core clinical criteria
        </p>
    </div>


    <div class="ml-2 sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
        <div>
            <label class="text-base font-semibold text-gray-900"><span class="counter subcounterreset"></span>Check all criteria that apply in Q4.</label><br />
        </div>
        <div class="mt-4 sm:col-span-2 sm:mt-0">
            <fieldset class="subsubcounterreset">
                <div class="space-y-1">
                    <div class="relative flex items-start">
                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                        <div class="flex h-6 items-center">
                            @Html.CheckBox("D1a.MCICRITCLN", Model.D1a.MCICRITCLN, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                        </div>
                        <div class="ml-3 text-sm leading-6">
                            <label asp-for="D1a.MCICRITCLN" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.MCICRITCLN)</label>
                        </div>
                    </div>
                    <div class="relative flex items-start">
                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900t"></span>
                        <div class="flex h-6 items-center">
                            @Html.CheckBox("D1a.MCICRITIMP", Model.D1a.MCICRITIMP, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                        </div>
                        <div class="ml-3 text-sm leading-6">
                            <label asp-for="D1a.MCICRITIMP" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.MCICRITIMP)</label>
                        </div>
                    </div>
                    <div class="relative flex items-start">
                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                        <div class="flex h-6 items-center">
                            @Html.CheckBox("D1a.MCICRITFUN", Model.D1a.MCICRITFUN, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                        </div>
                        <div class="ml-3 text-sm leading-6">
                            <label asp-for="D1a.MCICRITFUN" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.MCICRITFUN)</label>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>

<div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6 subcounterreset">
    <label asp-for="D1a.MCI"><span>4b.</span> @Html.DisplayNameFor(m => m.D1a.MCI)</label>
    <div class="mt-4 sm:col-span-2 sm:mt-0">
        <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.MCI"></p>
        <radio-button-group id="@Html.IdFor(m => m.D1a.MCI)" for="@Model.D1a.MCI" items="Model.MCIListItems" ui-behaviors="Model.MCIUIBehavior"></radio-button-group>
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.MCI"></span>
        <submission-error location="MCI" errors="Model.BaseForm.UnresolvedErrors" />
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.MCICriteriaValidation"></span>
        <submission-error location="MCICriteriaValidation" errors="Model.BaseForm.UnresolvedErrors" />
    </div>
    <p class="italic text-gray-500">If all three criteria are checked, choose <strong>1=MCI</strong> for Q4b. If less than 3 criteria are met, choose <strong>0=No</strong> for Q4b.</p>
</div>

<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
        <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
            Cognitively impaired, not MCI/dementia
        </p>
    </div>
    <p class="mt-2 text-sm text-gray-900">
        The purpose of the “Cognitively impaired, not MCI/dementia” category is to capture those individuals with evidence of cognitive
        impairment or decline who do not meet formal MCI criteria.
    </p>
    <p class="mt-2 text-sm text-gray-900">
        <strong>Check all applicable criteria for cognitively impaired, not MCI/dementia in Q5</strong>, <i>using any relevant data</i>. Any conditions
        contributing to impairment <i>(e.g., substance abuse or medications)</i> should be identified in Section 3.
    </p>
    <i>(Note: If recent onset (not longstanding impairment), indicate the cognitive symptom(s) in Form B9 – Clinician Judgment of Symptoms.)</i>
</div>

<div class="ml-2 sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
    <div>
        <label class="text-base font-semibold text-gray-900">
            <span class="counter"></span>Check all applicable criteria for cognitively impaired, not MCI/dementia in Q5, using any relevant data. Any conditions
            contributing to impairment (e.g., substance abuse or medications) should be identified in Section 3.
        </label><br />
    </div>
    <div class="mt-4 sm:col-span-2 sm:mt-0">
        <fieldset class="subsubcounterreset">
            <div class="space-y-1">
                <div class="relative flex items-start">
                    <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                    <div class="flex h-6 items-center">
                        @Html.CheckBox("D1a.IMPNOMCIFU", Model.D1a.IMPNOMCIFU, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                    </div>
                    <div class="ml-3 text-sm leading-6">
                        <label asp-for="D1a.IMPNOMCIFU" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.IMPNOMCIFU)</label>
                    </div>
                </div>
                <div class="relative flex items-start">
                    <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                    <div class="flex h-6 items-center">
                        @Html.CheckBox("D1a.IMPNOMCICG", Model.D1a.IMPNOMCICG, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                    </div>
                    <div class="ml-3 text-sm leading-6">
                        <label asp-for="D1a.IMPNOMCICG" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.IMPNOMCICG)</label>
                    </div>
                </div>
                <div class="relative flex items-start">
                    <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                    <div class="flex h-6 items-center">
                        @Html.CheckBox("D1a.IMPNOMCLCD", Model.D1a.IMPNOMCLCD, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                    </div>
                    <div class="ml-3 text-sm leading-6">
                        <label asp-for="D1a.IMPNOMCLCD" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.IMPNOMCLCD)</label>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-2 sm:items-start sm:gap-4 sm:py-1">
                    <div class="relative flex items-start">
                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                        <div class="flex h-6 items-center">
                            @Html.CheckBox("D1a.IMPNOMCIO", Model.D1a.IMPNOMCIO, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none", data_affects = "true", data_affects_toggle_targets = "[ \"D1a.IMPNOMCIOX\" ]" })
                        </div>
                        <div class="ml-3 text-sm leading-6">
                            <label asp-for="D1a.IMPNOMCIO" class="font-medium text-gray-900">@Html.DisplayNameFor(m => m.D1a.IMPNOMCIO)</label>
                        </div>
                    </div>
                    <div class="relative flex items-start">
                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                        <div class="mt-2">
                            <label asp-for="D1a.IMPNOMCIOX">@Html.DisplayNameFor(m => m.D1a.IMPNOMCIOX)</label>
                            <input asp-for="D1a.IMPNOMCIOX" />
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.IMPNOMCIOX"></span>
                            <submission-error location="IMPNOMCIOX" errors="Model.BaseForm.UnresolvedErrors" />
                        </div>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
</div>

<div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
    <label asp-for="D1a.IMPNOMCI">
        <span>5b.</span> @Html.DisplayNameFor(m => m.D1a.IMPNOMCI)
    </label>
    <div class="mt-4 sm:col-span-2 sm:mt-0">
        <p class="text-sm text-gray-500" asp-description-for="@Model.D1a.IMPNOMCI"></p>
        <radio-button-group id="@Html.IdFor(m => m.D1a.IMPNOMCI)" for="@Model.D1a.IMPNOMCI" ui-behaviors="Model.IMPNOMCIUIBehavior" items="Model.IMPNOMCIListItems"></radio-button-group>
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.IMPNOMCI"></span>
        <submission-error location="IMPNOMCI" errors="Model.BaseForm.UnresolvedErrors" />
        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.IMPNOMCICriteriaValidation"></span>
        <submission-error location="IMPNOMCICriteriaValidation" errors="Model.BaseForm.UnresolvedErrors" />
    </div>
    <span class="text-gray-500 italic">
        If any of the criteria in Q5 are met, or if only some of the MCI criteria from Q4 are met, choose 1=Yes for Q5b. Note, if <strong><u>only</u></strong> the third
        MCI criteria is met in Q4, select 0=No for Q5b.
    </span>
</div>



<div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
    <div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
        <div class="-ml-2 -mt-2 flex flex-wrap items-baseline bg-indigo-100">
            <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">
                Affected Domains – Dementia and MCI
            </p>
        </div>
        <span class="counter invisible"></span>
        <p class="mt-2 text-sm text-gray-900">
            Choose domains that are impaired at the current visit based on clinical judgment informed by clinical history and
            neuropsychological testing. <u>Select one or more</u> as <strong>Impaired</strong>; all others will default to <strong>unimpaired</strong> in the NACC database.
        </p>
        <p class="mt-2 text-sm text-gray-900">
            Note on <strong>behavior changes:</strong> For patients with <i>dementia</i> who have behavior changes, record the presence of behavioral changes here
            (not in the following MBI section) by marking Q6f as <strong>Impaired</strong> and Q7 as <strong>0 = No.</strong> For behavioral changes in the context of an MCI (or
            as an isolated) symptom, consider a diagnosis of MBI in the next section.
        </p>
    </div>
    <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.AffectedDomainsIndicated"></span>
    <submission-error location="AffectedDomainsIndicated" errors="Model.BaseForm.UnresolvedErrors" />
    <table class="ml-2 table-auto min-w-full divide-y divide-gray-300">
        <thead>
            <tr class="odd:bg-white even:bg-gray-50 border-b">
                <th class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0"></th>
                <th class="w-8 py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Impaired</th>
            </tr>
        </thead>
        <tbody class="subcounterreset divide-y divide-gray-200">
            <tr class="odd:bg-white even:bg-gray-50 border-b">
                <td>
                    <label asp-for="D1a.CDOMMEM"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1a.CDOMMEM)</label>
                    <p class="text-sm text-gray-500" asp-description-for="D1a.CDOMMEM"></p>
                    <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.CDOMMEM"></span>
                    <submission-error location="CDOMMEM" errors="Model.BaseForm.UnresolvedErrors" />
                </td>
                <td>
                    <fieldset>
                        <div class="space-y-5">
                            <div class="relative flex items-start">
                                <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                <div class="flex h-6 items-center">
                                    @Html.CheckBox("D1a.CDOMMEM", Model.D1a.CDOMMEM, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                                </div>
                                <div class="ml-3 text-sm leading-6">
                                    <label for="CDOMMEM" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </td>
            </tr>
            <tr class="odd:bg-white even:bg-gray-50 border-b">
                <td>
                    <label asp-for="D1a.CDOMLANG"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1a.CDOMLANG)</label>
                    <p class="text-sm text-gray-500" asp-description-for="D1a.CDOMLANG"></p>
                    <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.CDOMLANG"></span>
                    <submission-error location="CDOMLANG" errors="Model.BaseForm.UnresolvedErrors" />
                </td>
                <td>
                    <fieldset>
                        <div class="space-y-5">
                            <div class="relative flex items-start">
                                <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                <div class="flex h-6 items-center">
                                    @Html.CheckBox("D1a.CDOMLANG", Model.D1a.CDOMLANG, new { @class = "h-4 w-4 rounded border-gray-400 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none" })
                                </div>
                                <div class="ml-3 text-sm leading-6">
                                    <label for="D1a_CDOMLANG" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </td>
            </tr>
            <tr class="odd:bg-white even:bg-gray-50 border-b">
                <td>
                    <label asp-for="D1a.CDOMATTN"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1a.CDOMATTN)</label>
                    <p class="text-sm text-gray-500" asp-description-for="D1a.CDOMATTN"></p>
                    <span class="mt-2 text-sm text-red-600" asp-validation-for="D1a.CDOMATTN"></span>
                    <submission-error location="CDOMATTN" errors="Model.BaseForm.UnresolvedErrors" />
                </td>
                <td>
                    <fieldset>
                        <div class="space-y-5">
                            <div class="relative flex items-start">
                                <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                <div class="flex h-6 items-center">
