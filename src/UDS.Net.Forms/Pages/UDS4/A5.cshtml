        <!--// Neurologic conditions //-->
        <div>
            <table class="table-auto min-w-full">
                <thead>
                    <tr>
                        <th class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">
                            <h2 class="text-base font-semibold leading-7 text-gray-900">
                                <span class="counter"></span> Neurologic conditions
                            </h2>
                        </th>
                        @foreach (var item in Model.ConditionsListItems)
                        {
                            <th class="py-3.5 pl-4 pr-3 text-center text-sm font-semibold text-gray-900 sm:pl-0">@item.Text</th>
                        }
                    </tr>
                </thead>
                <tbody class="subcounterreset">
                    <tr>
                        <td>
                            <label asp-for="A5.PD"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.PD)</label>
                            <p class="text-sm text-gray-500" asp-description-for="A5.PD"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.PD"></span>
                        </td>
                        <td>
                            <div class="relative flex items-start">
                                <div class="flex items-center">
                                    <input class="h-4 border-gray-300 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none"
                                           data-affects="true"
                                           data-affects-targets="[ {&quot;A5.PDYR&quot;: {&quot;disabled&quot;: &quot;true&quot;} } ]"
                                           id="A5_PD[0]" name="A5.PD" type="radio" value="0" @(Model.A5.PD == 0 ? "checked" : "")>
                                    <label class="ml-3 block text-sm font-medium leading-6 text-gray-900" for="A5_PD[0]">
                                        <span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">@Model.ConditionsLimitedListItems[0].Value</span>
                                    </label>
                                </div>
                            </div>
                        </td>
                        <td>
                            <div class="relative flex items-start">
                                <div class="flex items-center">
                                    <input class="h-4 border-gray-300 text-indigo-600 focus:ring-indigo-600 disabled:bg-slate-50 disabled:text-slate-500 disabled:border-slate-200 disabled:shadow-none"
                                           data-affects="true"
                                           data-affects-targets="[ {&quot;A5.PDYR&quot;: {&quot;disabled&quot;: &quot;false&quot;} } ]"
                                           id="A5_PD[1]" name="A5.PD" type="radio" value="1" @(Model.A5.PD == 1 ? "checked" : "")>
                                    <label class="ml-3 block text-sm font-medium leading-6 text-gray-900" for="A5_PD[1]">
                                        <span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">@Model.ConditionsLimitedListItems[1].Value</span>
                                    </label>
                                </div>
                            </div>
                        </td>
                        <td></td>
                        <td>
