                                <p class="text-sm text-gray-500" asp-description-for="A5.CVBYPASS"></p>
                                <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.CVBYPASS"></span>
                            </td>
                            <table-cell-radio-buttons id="@Html.IdFor(m => m.A5.CVBYPASS)" for="@Model.A5.CVBYPASS" items="Model.ConditionsListItems"></table-cell-radio-buttons>
                        </tr>
                        <tr>
