                    <thead>
                        <tr>
                            <th class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">
                                <h2 class="text-base font-semibold leading-7 text-gray-900">
                                    <span class="counter"></span> Cardiovascular disease
                                </h2>
                            </th>
                            @foreach (var item in Model.ConditionsListItems)
                            {
                                <th class="py-3.5 pl-4 pr-3 text-center text-sm font-semibold text-gray-900 sm:pl-0">@item.Text</th>
