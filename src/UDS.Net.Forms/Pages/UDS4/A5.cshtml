@page "{id:int?}"
@model UDS.Net.Forms.Pages.UDS4.A5Model
@{
    Layout = "_LayoutForm";
    string rowCSS = "sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6";
}

<div>
    <p class="mt-1 text-sm text-gray-500">
        INSTRUCTIONS: This form is to be completed by the clinician or ADC staff. For additional clarification and examples, see <guidebook kind="@Model.Visit.Kind"></guidebook>, Form A5. Check only <span class="underline">one</span> box per question.
    </p>
</div>
<div>
    <div class="mt-10 space-y-8 border-b border-gray-900/10 pb-12 sm:space-y-0 sm:divide-y sm:divide-gray-900/10 sm:border-t sm:pb-0">

        <!--// Smoking and alcohol use //-->
        <div>
            <h2 class="text-base font-semibold leading-7 text-gray-900">
                <span class="counter"></span> History of cigarette smoking and alcohol use
            </h2>
            <div class="ml-2 subcounterreset">
                <!--// CIGARETTE SMOKING //-->
                <h2 class="text-base font-semibold leading-8 text-indigo-300">CIGARETTE SMOKING</h2>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.TOBAC30"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.TOBAC30)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.TOBAC30"></p>
                        <radio-button-group id="@Html.IdFor(m => m.A5.TOBAC30)" for="@Model.A5.TOBAC30" items="Model.BasicYesNoListItems"></radio-button-group>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.TOBAC30"></span>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.TOBAC100"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.TOBAC100)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.TOBAC100"></p>
                        <radio-button-group id="@Html.IdFor(m => m.A5.TOBAC100)" for="@Model.A5.TOBAC100" items="Model.TOBAC100ListItems" ui-behaviors="Model.TOBAC100UIBehavior"></radio-button-group>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.TOBAC100"></span>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.SMOKYRS"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.SMOKYRS)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.SMOKYRS"></p>
                        <input asp-for="@Model.A5.SMOKYRS" />
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.SMOKYRS"></span>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.PACKSPER"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.PACKSPER)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.PACKSPER"></p>
                        <radio-button-group id="@Html.IdFor(m => m.A5.PACKSPER)" for="@Model.A5.PACKSPER" items="Model.SmokingFrequencyListItems"></radio-button-group>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.PACKSPER"></span>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.QUITSMOK"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.QUITSMOK)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.QUITSMOK"></p>
                        <input asp-for="@Model.A5.QUITSMOK" />
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.QUITSMOK"></span>
                    </div>
                </div>
                <!--// ALCOHOL USE //-->
                <h2 class="text-base font-semibold leading-8 text-indigo-300">ALCOHOL USE</h2>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.ALCOCCAS"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.ALCOCCAS)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.ALCOCCAS"></p>
                        <radio-button-group id="@Html.IdFor(m => m.A5.ALCOCCAS)" for="@Model.A5.ALCOCCAS" items="Model.ALCOCCASListItems" ui-behaviors="Model.ALCOCCASUIBehavior"></radio-button-group>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.ALCOCCAS"></span>
                    </div>
                </div>
                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                    <label asp-for="A5.ALCFREQ"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.A5.ALCFREQ)</label>
                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.ALCFREQ"></p>
                        <radio-button-group id="@Html.IdFor(m => m.A5.ALCFREQ)" for="@Model.A5.ALCFREQ" items="Model.AlcoholUseFrequencyListItems"></radio-button-group>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.ALCFREQ"></span>
                    </div>
                </div>
            </div>

        </div>

        <!--// Cardiovascular disease //-->
        <div>
