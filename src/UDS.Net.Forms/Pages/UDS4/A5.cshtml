                                <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                                    <label asp-for="A5.PDYR"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.A5.PDYR)</label>
                                    <div class="mt-4 sm:col-span-2 sm:mt-0">
                                        <p class="text-sm text-gray-500" asp-description-for="@Model.A5.PDYR"></p>
                                        <input asp-for="@Model.A5.PDYR" />
                                        <span class="mt-2 text-sm text-red-600" asp-validation-for="A5.PDYR"></span>
                                    </div>
                                </div>
                            </div>
                        </td>
                    </tr>
