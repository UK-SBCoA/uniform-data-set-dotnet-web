            <p class="italic mt-2 text-sm text-gray-700">Reported by participant</p>
        </div>
        <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
            <label asp-for="B9.DECCOG"><span class="counter"></span> @Html.DisplayNameFor(m => m.B9.DECCOG)</label>
