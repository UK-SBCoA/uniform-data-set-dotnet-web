@page  "{id:int?}"
@model UDS.Net.Forms.Pages.UDS3.D1Model
@{
    Layout = "_LayoutForm";
    string rowCSS = "sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6";
}

<div>
    <p class="mt-1 text-sm text-gray-500">
        INSTRUCTIONS: This form is to be completed by the clinician. For additional clarification and examples, see <guidebook kind="@Model.Visit.Kind"></guidebook>, Form D1. Check only <span class="underline">one</span> box per question.
    </p>
</div>
<div class="divide-y divide-gray-200 overflow-hidden rounded-lg bg-white shadow">
    <div class="px-4 py-5 sm:px-6">
        This form is divided into three main sections
    </div>
    <div class="border-t border-gray-100">
        <dl class="divide-y divide-gray-100">
            <div class="px-4 py-6 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-gray-900">
                    <p class="text-sm text-gray-500">Section 1</p>
                    <h3 class="mt-1 font-semibold text-indigo-900">
                        <a href="#section-1">Cognitive and behavioral status</a>
                    </h3>
                </dt>
                <dd class="mt-1 text-sm leading-6 text-gray-700 sm:col-span-2 sm:mt-0">Normal cognition / MCI / dementia and dementia syndrome</dd>
            </div>
            <div class="px-4 py-6 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-gray-900">
                    <p class="text-sm text-gray-500">Section 2</p>
                    <h3 class="mt-1 font-semibold text-indigo-900">
                        <a href="#section-2">Biomarkers, imaging, and genetics</a>
                    </h3>
                </dt>
                <dd class="mt-1 text-sm leading-6 text-gray-700 sm:col-span-2 sm:mt-0">Neurodegenerative imaging and CSF biomarkers, imaging evidence for CVD, and known genetic mutations for AD and FTLD</dd>
            </div>
            <div class="px-4 py-6 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-gray-900">
                    <p class="text-sm text-gray-500">Section 3</p>
                    <h3 class="mt-1 font-semibold text-indigo-900">
                        <a href="#section-3">Etiological diagnoses</a>
                    </h3>
                </dt>
                <dd class="mt-1 text-sm leading-6 text-gray-700 sm:col-span-2 sm:mt-0">Presumed etiological diagnoses for the cognitive disorder</dd>
            </div>
        </dl>
    </div>
</div>
<div>
    <div class="mt-10 space-y-8 border-b border-gray-900/10 pb-12 sm:space-y-0 sm:divide-y sm:divide-gray-900/10 sm:border-t sm:pb-0">
        <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
            <label asp-for="D1.DXMETHOD"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1.DXMETHOD)</label>
            <div class="mt-4 sm:col-span-2 sm:mt-0">
                <p class="text-sm text-gray-500" asp-description-for="@Model.D1.DXMETHOD"></p>
                <radio-button-group id="@Html.IdFor(m => m.D1.DXMETHOD)" for="@Model.D1.DXMETHOD" items="Model.DiagnosisMethodListItems"></radio-button-group>
                <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.DXMETHOD"></span>
            </div>
        </div>

        <div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
            <a name="section-1"></a>
            <div class="-ml-2 -mt-2 flex flex-wrap items-baseline">
                <h3 class="ml-2 mt-2 font-semibold text-sm text-gray-500 uppercase">Section 1</h3>
                <p class="ml-2 mt-1 truncate font-semibold leading-6 text-indigo-900">Cognitive and behavioral status</p>
            </div>
        </div>

        <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
            <label asp-for="D1.NORMCOG"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1.NORMCOG)</label>
            <div class="mt-4 sm:col-span-2 sm:mt-0">
                <p class="text-sm text-gray-500" asp-description-for="@Model.D1.NORMCOG"></p>
                <radio-button-group id="@Html.IdFor(m => m.D1.NORMCOG)" for="@Model.D1.NORMCOG" items="Model.NORMCOGListItems" ui-behaviors="Model.NORMCOGUIBehavior"></radio-button-group>
                <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.NORMCOG"></span>
            </div>
        </div>

        <div class="p-2 sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6 bg-indigo-100">
            <p class="text-base font-semibold text-gray-900">All-cause dementia</p>
            <p class="mt-2 text-sm font-semibold text-gray-900">
                The participant has cognitive or behavioral (neuropsychiatric) symptoms that meet all of the following criteria:
            </p>
            <ul class="list-disc ml-4 text-sm">
                <li>Interfere with ability to function as before at work or at usual activities?</li>
                <li>Represent a decline from previous levels of functioning?</li>
                <li>Are not explained by delirium or major psychiatric disorder?</li>
                <li>
                    Include cognitive impairment detected and diagnosed through a combination of 1) history-taking and 2) objective cognitive assessment (bedside or neuropsychological testing)?
                </li>
            </ul>

            <p class="mt-2 text-sm font-semibold text-gray-900 underline">AND</p>
            <div class="ml-6">
                <p class="mt-2 text-sm font-semibold text-gray-900">
                    Impairment in one* or more of the following domains.
                </p>
                <ul class="list-[square] ml-4 text-sm">
                    <li>Impaired ability to acquire and remember new information</li>
                    <li>Impaired reasoning and handling of complex tasks, poor judgment</li>
                    <li>Impaired visuospatial abilities</li>
                    <li>Impaired language functions</li>
                    <li>Changes in personality, behavior, or comportment</li>
                </ul>
                <p class="mt-2 text-sm text-gray-700 italic">
                    * In the event of single-domain impairment (e.g., language in PPA, behavior in bvFTD, posterior cortical atrophy), the participant must not fulfill criteria for MCI.
                </p>
            </div>
        </div>

        <div class="sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
            <label asp-for="D1.DEMENTED"><span class="counter"></span> @Html.DisplayNameFor(m => m.D1.DEMENTED)</label>
            <div class="mt-4 sm:col-span-2 sm:mt-0">
                <p class="text-sm text-gray-500" asp-description-for="@Model.D1.DEMENTED"></p>
                <radio-button-group id="@Html.IdFor(m => m.D1.DEMENTED)" for="@Model.D1.DEMENTED" items="Model.DEMENTEDListItems" ui-behaviors="Model.DEMENTEDBehavior"></radio-button-group>
                <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.DEMENTED"></span>
            </div>
        </div>

        <div class="sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6" data-controller="fancycheckboxes">
            <label class="text-base font-semibold text-gray-900"><span class="counter"></span> If the participant meets criteria for dementia, ANSWER QUESTIONS 4a–4f below and then SKIP TO QUESTION 6.</label>
            <p class="text-sm text-gray-500">
                Based entirely on the history and examination (including neuropsychological testing), what is the cognitive/behavioral syndrome? <strong>Select one or more as Present; all others will default to Absent in the NACC database.</strong>
            </p>
            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.DementiaSyndromeIndicated"></span>
            <table class="ml-2 table-auto min-w-full divide-y divide-gray-300">
                <thead>
                    <tr>
                        <th class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Dementia syndrome</th>
                        <th class="w-8 py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Present</th>
                    </tr>
                </thead>
                <tbody class="subcounterreset divide-y divide-gray-200">
                    <tr>
                        <td>
                            <label asp-for="D1.AMNDEM"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.AMNDEM)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.AMNDEM"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.AMNDEM"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.AMNDEM", Model.D1.AMNDEM, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_AMNDEM" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label asp-for="D1.PCA"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.PCA)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.PCA"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.PCA"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.PCA", Model.D1.PCA, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_PCA" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label asp-for="D1.PPASYN"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.PPASYN)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.PPASYN"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.PPASYN"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.PPASYN", Model.D1.PPASYN, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600", data_affects = "true", data_affects_toggle_targets = "[ \"D1.PPASYNT\" ]" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_PPASYN" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2">
                            <div class="ml-2 sm:grid sm:grid-cols-3 sm:items-start sm:gap-4 sm:py-6">
                                <label asp-for="D1.PPASYNT"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.D1.PPASYNT)</label>
                                <div class="mt-4 sm:col-span-2 sm:mt-0">
                                    <p class="text-sm text-gray-500" asp-description-for="@Model.D1.PPASYNT"></p>
                                    <radio-button-group id="@Html.IdFor(m => m.D1.PPASYNT)" for="@Model.D1.PPASYNT" items="Model.PPASyndromeListItems"></radio-button-group>
                                    <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.PPASYNT"></span>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label asp-for="D1.FTDSYN"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.FTDSYN)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.FTDSYN"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.FTDSYN"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.FTDSYN", Model.D1.FTDSYN, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_FTDSYN" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label asp-for="D1.LBDSYN"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.LBDSYN)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.LBDSYN"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.LBDSYN"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.LBDSYN", Model.D1.LBDSYN, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_LBDSYN" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label asp-for="D1.NAMNDEM"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.NAMNDEM)</label>
                            <p class="text-sm text-gray-500" asp-description-for="D1.NAMNDEM"></p>
                            <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.NAMNDEM"></span>
                        </td>
                        <td>
                            <fieldset>
                                <div class="space-y-5">
                                    <div class="relative flex items-start">
                                        <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                        <div class="flex h-6 items-center">
                                            @Html.CheckBox("D1.NAMNDEM", Model.D1.NAMNDEM, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600" })
                                        </div>
                                        <div class="ml-3 text-sm leading-6">
                                            <label for="D1_NAMNDEM" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="ml-2 sm:grid sm:grid-cols-1 sm:items-start sm:gap-4 sm:py-6">
            <label class="text-base font-semibold text-gray-900"><span class="counter"></span> If the participant does not have normal cognition or behavior and is not clinically demented, indicate the type of cognitive impairment below.</label>

            <div class="p-4 bg-indigo-100">

                <p class="text-base font-semibold text-gray-900">MCI CORE CLINICAL CRITERIA</p>

                <ul class="list-disc ml-4 text-sm">
                    <li>Is the participant, the co-participant, or a clinician concerned about a change in cognition compared to the participant's previous level?</li>
                    <li>Is there impairment in one or more cognitive domains (memory, language, executive function, attention, and visuospatial skills)?</li>
                    <li>Is there largely preserved independence in functional abilities (no change from prior manner of functioning or uses minimal aids or assistance)?</li>
                </ul>
            </div>
        </div>

        <table class="ml-2 table-auto min-w-full divide-y divide-gray-300">
            <caption class="caption-top text-sm text-gray-500">
                Select one syndrome from 5a-5e as being Present (all others will default to Absent in the NACC database), and then <strong>CONTINUE TO QUESTION 6</strong>. If you select MCI below, it should meet the MCI core clinical criteria outlined above.
            </caption>
            <thead>
                <tr>
                    <th class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Type</th>
                    <th class="w-24 py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Present</th>
                    <th class="w-1/2 py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">Affected domains</th>
                    @foreach (var item in Model.SimpleNoYesListItems)
                    {
                        <th class="w-12 py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0">@item.Text</th>
                    }
                </tr>
            </thead>
            <tbody class="subcounterreset divide-y divide-gray-200">
                <tr>
                    <td>
                        <label asp-for="D1.MCIAMEM"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAMEM)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAMEM"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAMEM"></span>
                    </td>
                    <td>
                        <fieldset>
                            <div class="space-y-5">
                                <div class="relative flex items-start">
                                    <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                    <div class="flex h-6 items-center">
                                        @Html.CheckBox("D1.MCIAMEM", Model.D1.MCIAMEM, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600", })
                                    </div>
                                    <div class="ml-3 text-sm leading-6">
                                        <label for="D1_MCIAMEM" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </td>
                    <td colspan="3"></td>
                </tr>
            </tbody>
            <tbody class="subsubcounterreset">
                <tr>
                    <td>
                        <label asp-for="D1.MCIAPLUS"><span class="subcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAPLUS)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAPLUS"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAPLUS"></span>
                    </td>
                    <td>
                        <fieldset>
                            <div class="space-y-5">
                                <div class="relative flex items-start">
                                    <span class="mr-3 block text-sm font-medium leading-6 text-gray-900"></span>
                                    <div class="flex h-6 items-center">
                                        @Html.CheckBox("D1.MCIAPLUS", Model.D1.MCIAPLUS, new { @class = "h-4 w-4 rounded border-gray-300 text-indigo-600 focus:ring-indigo-600", data_affects = "true", data_affects_toggle_targets = "[ \"D1.MCIAPLAN\" , \"D1.MCIAPATT\" , \"D1.MCIAPEX\" ,\"D1.MCIAPVIS\" ]" })
                                    </div>
                                    <div class="ml-3 text-sm leading-6">
                                        <label for="D1_MCIAPLUS" class="font-medium text-gray-900"><span class="inline-flex items-center rounded-full bg-gray-100 px-2.5 py-0.5 text-xs font-medium text-gray-800">1</span></label>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </td>
                    <td colspan="3">
                        <label class="text-base font-semibold text-gray-900">CHECK YES for at least one additional domain (besides memory)</label>
                    </td>
                </tr>
                <tr>
                    <td>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.AmnesticMCIMultipleDomainsIndicated"></span>
                    </td>
                </tr>
                <tr>
                    <td colspan="2"></td>
                    <td>
                        <label asp-for="D1.MCIAPLAN"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAPLAN)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAPLAN"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAPLAN"></span>
                    </td>
                    <table-cell-radio-buttons id="@Html.IdFor(m => m.D1.MCIAPLAN)" for="@Model.D1.MCIAPLAN" items="Model.SimpleNoYesListItems"></table-cell-radio-buttons>
                </tr>
                <tr>
                    <td colspan="2"></td>
                    <td>
                        <label asp-for="D1.MCIAPATT"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAPATT)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAPATT"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAPATT"></span>
                    </td>
                    <table-cell-radio-buttons id="@Html.IdFor(m => m.D1.MCIAPATT)" for="@Model.D1.MCIAPATT" items="Model.SimpleNoYesListItems"></table-cell-radio-buttons>
                </tr>
                <tr>
                    <td colspan="2"></td>
                    <td>
                        <label asp-for="D1.MCIAPEX"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAPEX)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAPEX"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAPEX"></span>
                    </td>
                    <table-cell-radio-buttons id="@Html.IdFor(m => m.D1.MCIAPEX)" for="@Model.D1.MCIAPEX" items="Model.SimpleNoYesListItems"></table-cell-radio-buttons>
                </tr>
                <tr>
                    <td colspan="2"></td>
                    <td>
                        <label asp-for="D1.MCIAPVIS"><span class="subsubcounter"></span> @Html.DisplayNameFor(m => m.D1.MCIAPVIS)</label>
                        <p class="text-sm text-gray-500" asp-description-for="D1.MCIAPVIS"></p>
                        <span class="mt-2 text-sm text-red-600" asp-validation-for="D1.MCIAPVIS"></span>
                    </td>
                    <table-cell-radio-buttons id="@Html.IdFor(m => m.D1.MCIAPVIS)" for="@Model.D1.MCIAPVIS" items="Model.SimpleNoYesListItems"></table-cell-radio-buttons>
                </tr>
            </tbody>
            <tbody class="subsubcounterreset">
                <tr>
                    <td>
