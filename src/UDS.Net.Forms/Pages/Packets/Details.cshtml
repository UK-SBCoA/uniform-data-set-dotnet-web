@page "{id:int?}"
@model UDS.Net.Forms.Pages.Packets.DetailsModel
@{
    ViewData["Title"] = Model.PageTitle;
}


<div class="divide-y divide-gray-200 lg:grid lg:grid-cols-12 lg:divide-y-0 lg:divide-x">
    <div class="py-6 lg:col-span-3">
        @if (Model.Packet != null)
        {
            <nav class="space-y-1">
                <a asp-page="Index" class="bg-indigo-50 border-indigo-500 text-indigo-700 hover:bg-indigo-50 hover:text-indigo-700 group border-l-4 px-3 py-2 flex items-center text-sm font-medium">
                    Back
                </a>
            </nav>
        }

    </div>
    <div class="lg:col-span-9 px-5 py-6">
        <partial name="../PacketSubmissions/_Index" model="Model.Packet" />
    </div>
</div>
